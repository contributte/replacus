{
  "name": "contributte/replacus",
  "description": "Latte-based replacer for simple texts.",
  "keywords": [
    "latte",
    "replacer"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/contributte/replacus",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "latte/latte": "^2.5.1"
  },
  "require-dev": {
    "nette/tester": "^2.3.2",
    "nette/utils": "^3.0",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Replacus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\Contributte\\Replacus\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
