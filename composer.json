{
  "name": "contributte/replacus",
  "description": "Latte-based replacer for simple texts.",
  "keywords": [
    "latte",
    "replacer"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/contributte/replacus",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "latte/latte": "^2.5.1"
  },
  "require-dev": {
    "nette/utils": "^3.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Replacus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\Contributte\\Replacus\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
